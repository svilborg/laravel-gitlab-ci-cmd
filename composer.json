{
  "name": "svilborg/laravel-gitlab-ci-cmd",
  "description": "Laravel Gitlab CI Command",
  "keywords": [
    "framework",
    "laravel",
    "gitlab",
    "gitlab-ci",
    "pipelines",
    "jobs"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.*",
    "m4tthumphrey/php-gitlab-api": "^9.12",
    "php-http/guzzle6-adapter": "^1.0"
  },
  "require-dev": {
        "phpunit/phpunit": "^7.0",
        "barryvdh/laravel-ide-helper": "^2.5",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "GitlabCi\\": "src"
    }
  },
  "autoload-dev": {
     "psr-4": {
         "Tests\\": "tests/"
     }
  }
}
